{
    "name": "spool/apiinfo",
    "description": "PHP框架API文档生成工具, 无需生成静态文档. 动态生成. 目前先基于Laravel和Hyperf",
    "type": "library",
    "require": {
        "php": "^7.0"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "yydick",
            "email": "yydick@sohu.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Spool\\ApiInfo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spool\\ApiInfo\\ApiInfoProvider"
            ],
            "aliases": {
                "ApiInfo": "Spool\\ApiInfo\\ApiInfo"
            }
        }
    }
}
